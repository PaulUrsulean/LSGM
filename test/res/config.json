{
    "globals": {
        "seed": 42,
        "prior": null,
        "add_const": false,
        "eps": 1e-16
    },
    "training": {
        "gpu_id": null,
        "use_early_stopping": true,
        "early_stopping_mode": "min",
        "early_stopping_metric": "val_mse_loss",
        "early_stopping_patience": 50,
        "epochs": 500,
        "batch_size": 128,
        "load_path": null,
        "optimizer": {
            "type": "adam",
            "learning_rate": 0.0005,
            "betas": [
                0.9,
                0.999
            ]
        },
        "scheduler": {
            "stepsize": 200,
            "gamma": 0.5
        }
    },
    "data": {
        "timesteps": 49,
        "name": "springs",
        "path": "data",
        "random": {
            "atoms": 4,
            "dims": 100,
            "examples": 100,
            "timesteps": 200
        },
        "springs": {
            "suffix": "_springs5",
            "atoms": 5,
            "dims": 4
        }
    },
    "loss": {
        "beta": 1.0
    },
    "model": {
        "prediction_steps": 10,
        "factor_graph": true,
        "skip_first": false,
        "hard": false,
        "dynamic_graph": false,
        "temp": 0.5,
        "burn_in": false,
        "n_edge_types": 2,
        "encoder": {
            "model": "mlp",
            "hidden_dim": 256,
            "dropout": 0.5
        },
        "decoder": {
            "model": "rnn",
            "hidden_dim": 256,
            "dropout": 0.0,
            "prediction_variance": 5e-05
        }
    },
    "logging": {
        "log_step": 10,
        "log_dir": "./logs",
        "logger_config": "",
        "store_models": true
    }
}